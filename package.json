  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "start:pm2": "pm2 start dist/server/node-build.mjs --name revs-landing",
    "stop:pm2": "pm2 stop revs-landing",
    "restart:pm2": "pm2 restart revs-landing",
    "delete:pm2": "pm2 delete revs-landing",
    "logs:pm2": "pm2 logs revs-landing",
    "status:pm2": "pm2 status",
    "monit:pm2": "pm2 monit",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc --noEmit --skipLibCheck"
